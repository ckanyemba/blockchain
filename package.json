  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "parcel": "^2.12.0"
  },
  "dependencies": {
    "@types/react": "^18.3.8",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "elliptic": "^6.5.7",
